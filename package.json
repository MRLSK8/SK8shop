{
  "name": "sk8shop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "android:clean": "cd android && ./gradlew clean && cd ../",
    "android:build": "yarn android:clean && cd android && ./gradlew assembleRelease",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:detox": "detox build -c android.emu.debug",
    "test:detox": "detox test -c android.emu.debug",
    "firestore:emulator": "firebase emulators:start --only firestore",
    "codepush-staging-android": "appcenter codepush release-react -a mrlsk8/sk8shop -d Staging",
    "codepush-staging-ios": "appcenter codepush release-react -a mrlsk8/sk8shop-ios -d Staging"
  },
  "dependencies": {
