    "@types/lodash": "^4.14.182",
    "@types/react-native": "^0.69.5",
    "@types/react-native-vector-icons": "^6.4.11",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.1.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.3",
